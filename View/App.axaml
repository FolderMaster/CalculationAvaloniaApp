<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="View.App"
             RequestedThemeVariant="Default">
    <Application.Styles>
        <FluentTheme />
	</Application.Styles>
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="Assets/Converters.axaml"/>
				<ResourceInclude Source="Assets/DataTemplates.axaml"/>
				<ResourceInclude Source="Assets/Strings.axaml"/>
				<ResourceInclude Source="Assets/Images.axaml"/>
				<ResourceInclude Source="Views/RangeSlider.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
	<Application.DataTemplates>
		<StaticResource ResourceKey="ParameterTemplate"/>
		<StaticResource ResourceKey="ParametersCompositeTemplate"/>
		<StaticResource ResourceKey="AxisParametersSetTemplate"/>
	</Application.DataTemplates>
</Application>
